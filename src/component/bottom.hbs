<section>
    {{#with service}}
    <div class="col-12">
        <div class="row">
            <div class="card text-bg-success text-center">
                <h2 class="mt-3 mb-4">{{title}}</h2>
                <p>{{description}}</p>
                {{#each buttons}}
                <div class="my-2">
                    <button class="btn btn-primary" href="{{link}}">{{text}}</button>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    {{/with}}
</section>